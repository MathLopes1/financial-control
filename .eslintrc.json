{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "quotes": "off",
        "class-methods-use-this":"off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": "off",
        "camelcase": "off",
        "no-use-before-define": "off",
        "no-plusplus":"off",
        "eqeqeq":"off",
        "no-shadow":"off",
        "no-underscore-dangle":"off"        
    }
}
